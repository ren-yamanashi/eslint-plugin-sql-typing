{
  "name": "eslint-plugin-sql-typing",
  "version": "0.1.0",
  "description": "ESLint plugin for raw sql typing (Auto generate sql return type)",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "mysql",
    "sql",
    "typescript"
  ],
  "homepage": "https://github.com/ren-yamanashi/eslint-plugin-sql-typing#readme",
  "bugs": {
    "url": "https://github.com/ren-yamanashi/eslint-plugin-sql-typing/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ren-yamanashi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ren-yamanashi/eslint-plugin-sql-typing"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    "types": "./dist/index.d.mts",
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "setup": "test -f .env || cp default.env .env && pnpm install --frozen-lockfile && pnpm build",
    "build": "tsdown",
    "test": "vitest --run",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "check": "tsgo --noEmit",
    "test:e2e": "./scripts/e2e.sh"
  },
  "dependencies": {
    "mariadb": "^3.5.1",
    "node-sql-parser": "^5.4.0",
    "synckit": "^0.11.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@types/node": "^24.10.13",
    "@typescript-eslint/parser": "^8.56.0",
    "@typescript-eslint/rule-tester": "^8.56.0",
    "@typescript-eslint/utils": "^8.56.0",
    "@typescript/native-preview": "7.0.0-dev.20260212.1",
    "eslint": "^9.39.3",
    "eslint-plugin-import": "^2.32.0",
    "oxfmt": "^0.34.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0",
    "typescript": ">=4.8.4 <6.0.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "packageManager": "pnpm@10.29.3"
}
